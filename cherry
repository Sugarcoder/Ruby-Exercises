# sorting words

fruits = ["apples", "cherries", "oranges", "bananas"]

puts fruits.sort





# Table of Contents v2

title = "Fruits of the Spirit" 

chapter_names = ["love         page 1", 
	         "faith        page 2", 
	         "patience     page 3", 
	         "kindness     page 4", 
	         "goodness     page 5"]

puts title

puts chapter_names






# Roman Numeral & Return:

def roman_numeral one
	if one == 1
		return 'I'
	end
end


puts roman_numeral('one')






# Shuffle:

fruits = ["apple", "banana", "cherry"]

puts fruits.shuffle


